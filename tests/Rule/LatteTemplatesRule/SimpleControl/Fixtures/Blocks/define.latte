{define my-block, \stdClass $paramObject, string $paramString, ?string $paramNullable, $paramNoType, string $paramDefault = 'default', $paramNoTypeDefault = 'default', float $paramFloat = 1.23, int $paramInteger = 123, \stdClass[] $paramArray = [], array $paramArrayMixedDefault = [1, 2, 3.5, 'str1', [4, 5, 'str2']], bool $boolDefaultTrue = true, bool $boolDefaultFalse = false, ?string $nullableStringDefaultNull = null, string $stringDefaultClassConstant = Efabrica\PHPStanLatte\Compiler\NodeVisitor\Behavior\ExprTypeNodeVisitorInterface::TYPE_ATTRIBUTE}
